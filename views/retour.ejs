          <!-- <li class="nav-item active-pro ">
                <a class="nav-link" href="./upgrade.html">
                    <i class="material-icons">unarchive</i>
                    <p>Upgrade to PRO</p>
                </a>
            </li> -->
        </ul>
      </div>
    </div>

    <div class="main-panel">
          <!-- Navbar -->
      <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top ">
        <div class="container-fluid">
          <button class="navbar-toggler pull-right" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
            <span class="sr-only ">Toggle navigation</span>
            <span class="navbar-toggler-icon icon-bar"></span>
            <span class="navbar-toggler-icon icon-bar"></span>
            <span class="navbar-toggler-icon icon-bar"></span>
          </button>
        </div>
      </nav>
      <!-- End Navbar -->

      </br>
      <div class="container-fluid">
        <div class="row">
            <div class="col-xs-4  col-md-4"></div>

          <div class=" col-md-4">
            <div class="card">
              <div class="card-header card-header-primary">
                <h4 class="card-title">Retourner un livre</h4>
                <p class="card-category">Lire chaque jour ...</p>
              </div>
              <div class="card-body">
                <form action="/insertRetour" method="post" >

                  <div class="row">
                    <div class="col-md-12">
                      <div class="form-group">
                        <label class="bmd-label-floating">Etudiant</label>
                        <input id="student" list="students" name="student">
                        <datalist id="students">
                            <%  students.forEach(function(student,index) { %>
                                <option id="<%= student.cin %>" value="<%= student.cin %> - <%= student.nom %> <%= student.prenom %>">
                             <% }); %>
                        </datalist>
                      </div>
                    </div>
                  </div>

                  <div class="row">
                    <div class="col-md-12">
                      <div class="form-group">
                        <label class="bmd-label-floating">Livre&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</label>
                        <input id="book" list="books" name="book">
                        <datalist id="books">
                            <%  books.forEach(function(book,index) { %>
                                <option value="<%= book.idLivre %>-<%= book.titre %>">
                             <% }); %>
                        </datalist>
                      </div>
                    </div>
                  </div>


                  <% if(message == "ok"){ %>
                  <div class="alert alert-success">
                    <strong>Success!</strong> Retour effectué.
                  </div>
                  <% } %>
                  <center><button type="submit" id="btn"   class="btn btn-primary ">Effectuer le retour</button></center>
                  <div class="clearfix"></div>
                </form>

              </div>
            </div>
          </div>
        </div>
      </div>
      <footer class="footer">
        <div class="container-fluid">
          <center>
          <div class="copyright">
            &copy;

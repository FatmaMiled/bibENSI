        <a href="#" class="simple-text logo-normal">
           <img src="../img/logoENSI.png" width="40">BibENSI
        </a>
      </div>

      <div class="sidebar-wrapper">
        <ul class="nav">
          <li class="nav-item   ">
            <a class="nav-link" href=".././getAdminHomePage/admin">
              <i class="material-icons">dashboard</i>
